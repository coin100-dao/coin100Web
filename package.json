{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss}\"",
    "lint:css": "stylelint \"src/**/*.{css,scss}\" --fix",
    "lint:js": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "lint": "npm run lint:css && npm run lint:js",
    "build": "tsc --project tsconfig.app.json && vite build",
    "dev": "vite --host 0.0.0.0",
    "preview": "npm run dev && code --new-window --enable-preview"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{css,scss}": [
      "prettier --write",
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@hookform/resolvers": "^3.9.1",
    "@mui/icons-material": "^6.1.6",
    "@mui/lab": "^6.0.0-beta.18",
    "@mui/material": "^6.1.6",
    "@mui/x-charts": "^7.23.0",
    "@reduxjs/toolkit": "^2.3.0",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.7.7",
    "chart.js": "^4.4.7",
    "chartjs-adapter-date-fns": "^3.0.0",
    "jwt-decode": "^4.0.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.1",
    "react-plaid-link": "^3.6.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.27.0",
    "redux-persist": "^6.0.0",
    "web3": "^4.15.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@stylelint/prettier-config": "^3.0.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^9.14.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.13.0",
    "vite": "^5.4.10"
  }
}
